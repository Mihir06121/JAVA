import java.lang.*;
import java.io.*;

class Binary7
{
  public static void main(String args[])
  {
    int num = 0b1010;
    System.out.printf("\nIn Decimal = %d", num);
    System.out.printf("\nIn Octal = %o", num);
    System.out.printf("\nIn Hex = %x", num);
    System.out.printf("\nIn Octal = %s", Integer.toBinaryString(num));
  
  }
       
}

